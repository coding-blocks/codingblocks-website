{{> scripts}}

<script>
    $(function () {
        const registerLinkAdvanced = $('#payment-advanced-link'),
                registerLinkCombo = $('#payment-combo-link'),
                registerBoxAdvanced = $('#advanced-bundle-registration'),
                registerBoxCombo = $('#complete-bundle-registration')
        ;

        registerBoxAdvanced.hide()

        registerLinkAdvanced.click(function () {
            registerBoxCombo.hide()
            registerBoxAdvanced.show()
        })

        registerLinkCombo.click(function () {
            registerBoxAdvanced.hide()
            registerBoxCombo.show()
        })

    })
</script>
<script>
    $(document).ready(function () {
        var hash = window.location.hash;
        if (hash === '#advance' || hash === '#advanced') {
            $('#tab-advanced').toggleClass('active');
            $('#tab-complete').toggleClass('active');
            $('#advanced').toggleClass('active');
            $('#complete').toggleClass('active');
        }
        $('#ifOnlineCourse').attr('href','https://online.codingblocks.com/courses/26#runs')
        $('#ifOnlineCourse').attr('target','_blank')
    })
</script>
