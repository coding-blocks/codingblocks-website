  <script>
    const centerToggle = document.querySelector("#center-toggle")
    const coursesNoida = document.querySelector("#courses-noida")    
    const coursesPitampura = document.querySelector("#courses-pitampura")  

    coursesPitampura.classList.add("d-none")

    centerToggle.addEventListener("click", () => {
      if (centerToggle.innerHTML === "Pitampura") {
        centerToggle.innerHTML = "Noida"
        coursesNoida.classList.remove("d-none")
        coursesPitampura.classList.add("d-none")
      } else {
        centerToggle.innerHTML = "Pitampura"
        coursesNoida.classList.add("d-none")
        coursesPitampura.classList.remove("d-none")
      }
    })
  </script>

