<!DOCTYPE html>
<html lang="en">
<head>
    <title>{{title}}</title>
    {{> meta}}
    {{> head}}
    {{> courseSchema}}
</head>
<body class="scroll-assist">
{{> navbar}}
<div class="main-container">
    {{> header}}
    {{> overview}}
	<a id="highlights" class="in-page-link" data-scroll-id="course-features"></a>
	<section class="bg-dark">
		<div class="container">
			<div class="row">
				{{#each courseHighlights}}
					<div class="text-center col-sm-4">
						<div class="feature">
							<i class="icon fade-3-4 inline-block mb16 {{{icon}}}"></i>
							<h3>{{{title}}}</h3>
							<p>{{{content}}}</p>
						</div>
					</div>
				{{/each}}
			</div>
		</div>
	</section>
	{{> freeAccessBanner}}

	<a id="contents" class="in-page-link"></a>
    <section>
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <h2 class="uppercase mb64 text-center color-primary">Course Contents</h2>
                    <div>
						<div class="course-card-container">
							<div class="content-row">
								<div class="course-card col-xs-12 col-sm-6 col-md-4 col-lg-4">
									<h3 class="topic">Recursion and Optimization</h3>
									<hr>
									<ul class="points-list">
										<li>Recursion</li>
										<li>Backtracking</li>
										<li>Bitmasking</li>
									</ul>
								</div>
								<div class="course-card col-xs-12 col-sm-6 col-md-4 col-lg-4">
									<h3 class="topic">Data Structures and Algorithms</h3>
									<hr>
									<ul class="points-list">
										<li>Stacks and Queues – Adva Questions</li>
										<li>Linked Lists – Advanced Questions</li>
										<li>Generic Trees</li>
										<li>Binary Trees</li>
										<li>Binary Search Trees</li>
									</ul>
								</div>
								<div class="course-card col-xs-12 col-sm-6 col-md-4 col-lg-4">
									<h3 class="topic">Non Linear Data Structures</h3>
									<hr>
									<ul class="points-list">
										<li>Heaps</li>
										<li>Hash Tables</li>
										<li>Generics</li>
										<li>Graphs – Traversals, Minimum Spanning trees, Shortest path
											algorithms
										</li>
										<li>Text Processing – Text Matching, Compression</li>
										<li>Dynamic Programming</li>
									</ul>
								</div>
							</div>
						</div>
						<section>
							<div class="col-md-10 col-md-offset-1 complete-course-box bg-secondary">
								<h2 class="heading">Enroll in Complete Java Course</h2>
								<p class="description">Embark upon this journey from being an amateur coder to a DS-Algo expert with our complete Java course.</p>
								<a class="btn btn-filled" target="_blank"
									href="/classroom-courses/java-for-beginners.html">Register Now</a>
							</div>
						</section>
						<section style="">
							<div class="container">
								<div class="row v-align-children">
									<div class="col-md-4 col-sm-6">
										<div class="pricing-table pt-1 text-center emphasis">
											<h5 class="uppercase">
												Regular Batch
											</h5>
											<div><span class="price">₹12,000</span></div>
											<p class="lead">(12-15 lectures)</p>
											<a class="btn btn-white btn-lg" href="#register">Enroll Now</a>
											<p>
												<a href="/contactus">Contact Us</a>
												<br>Call(Toll-free): 1800-2744-504
											</p>
										</div>
									</div>
								</div>

							</div>
						</section>
						{{> courseschedule}}
						<a id="register" class="in-page-link"></a>
						<section>
							<div class="text-center">
								<h3>REGISTER NOW</h3>
							<div class="container">
					<div class="course-tabs">
						<ul class="nav nav-tabs" role="tablist">
							<li role="presentation" class="wide-tab active" id="tab-pitampura"><a href="#pitampura" aria-controls="profile" role="tab" data-toggle="tab" aria-expanded="false">Pitampura</a></li>
							<li role="presentation" class="wide-tab" id="tab-dwarka"><a href="#dwarka" aria-controls="profile" role="tab" data-toggle="tab" aria-expanded="false">Dwarka</a></li>
							<li role="presentation" class="wide-tab" id="tab-noida"><a href="#noida" aria-controls="profile" role="tab" data-toggle="tab" aria-expanded="false">Noida (Early bird discount)</a></li>
						</ul>
						<div class="tab-content">
							<div role="tabpanel" class="tab-pane active" id="pitampura">
								<div class="container widget-container" data-widget-id="{{frame-id-pitampura-lp}}">
									<iframe class="payment-widget" {{#if frame-id-pitampura-lp}}id="{{frame-id-pitampura-lp}}"{{/if}} frameborder="0" src="{{{registerLinkPitampura}}}"></iframe>
								</div>
							</div>
							<div role="tabpanel" class="tab-pane" id="dwarka">
								<div class="container widget-container" data-widget-id="{{frame-id-dwarka-lp}}">
									<iframe class="payment-widget" {{#if frame-id-dwarka-lp}}id="{{frame-id-dwarka-lp}}"{{/if}} frameborder="0" src="{{{registerLinkDwarkaLP}}}"></iframe>
								</div>
							</div>
							<div role="tabpanel" class="tab-pane" id="noida">
								<div class="container widget-container" data-widget-id="{{frame-id-noida}}">
									<iframe class="payment-widget" {{#if frame-id-noida}}id="{{frame-id-noida}}"{{/if}} frameborder="0" src="{{{registerLinkNoida}}}"></iframe>
								</div>
							</div>
						</div>
					</div>
				</div>
						</section>

                    </div>
                </div>
            </div>
        </div>

    </section>

    {{> team team=mentors}}
    {{> coursefaq}}
    {{> review-course-section}}
    {{> map}}
    {{> footer}}
</div>
{{> scripts}}
</body>
</html>
