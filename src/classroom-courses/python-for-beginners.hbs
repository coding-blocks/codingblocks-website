<!DOCTYPE html>
<html lang="en">
<head>
    <title>{{title}}</title>
    {{> meta}}
    {{> head}}
    {{> courseSchema}}
</head>
<body class="scroll-assist">
{{> navbar}}
<div class="main-container">
    {{> header}}
    {{> overview}}
    {{> coursehighlights}}
	{{> coursecontents}}
    {{> team team=mentors}}
	{{> courseenroll}}
	{{> courseschedule}}
    <section>
                                    <div class="text-center">
                                        <h3>REGISTER NOW</h3></div>
                                    <div class="container">
                                        <div class="course-tabs">
                                            <ul class="nav nav-tabs" role="tablist">
                                                <li role="presentation" class="wide-tab active" id="tab-delhi"><a href="#delhi" aria-controls="home" role="tab" data-toggle="tab" aria-expanded="true">Delhi</a></li>
                                                <li role="presentation" class="wide-tab" id="tab-dehradun"><a href="#dehradun" aria-controls="profile" role="tab" data-toggle="tab" aria-expanded="false">Dehradun (50% inaugural discount)</a></li>
                                            </ul>
                                            <div class="tab-content">
                                                <div role="tabpanel" class="tab-pane active" id="delhi">
                                                    <div class="col-xs-12 text-center bg-secondary" style="height: 100%;">
                                                        <p class="uppercase" style="margin-top: 100px;margin-bottom: 100px;">Seats full. Admissions closed.</p>
                                                    </div>
                                                </div>
                                                <div role="tabpanel" class="tab-pane" id="dehradun">
                                                    <iframe class="payment-widget" {{#if frame-id-dehradun}}id="{{frame-id-dehradun}}"{{/if}}
                                                            frameborder="0"
                                                            src="{{{registerLinkDehradun}}}"></iframe>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </section>
    {{> coursefaq}}
    {{> map}}
    {{> footer}}
</div>
{{> scripts}}
</body>
</html>
